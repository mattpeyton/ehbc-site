* {
  margin: 0;
  padding: 0;
}

/****
BASE STYLING
****/
.landing {
  background-image: url("../img/cityscape.jpg");
  background-position: 0 0;
  background-repeat: no-repeat;
  background-attachment: fixed;
  background-size: cover;
  height: 100vh;
  display: block;
}

header {
  display: flex;
  justify-content: space-between;
  width: 98vw;
  max-width: 1400px;
  margin: auto;
}

#logo {
  max-height: 100px;
  padding: 20px;
}

.nav-line {
  display: block;
  height: 3px;
  width: 100%;
  background-color: rgba(0,0,0,.1)
}

.navbar {
  display: inline-block;
  width: 70%;
  max-width: 1000px;
  padding-top: 55px;
}

.menuButton {
  display: inline-block;
  width: 150px;
  height: 100px;
  background: rgba(255,255,255,.2);
  margin: 20px;
  text-align: center;
  line-height: 100px;
  cursor: pointer;
}

.nav-list {
  width: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  margin: .5em 0;
  margin-left: -40px;

}


.nav-item {
  display: inline-block;
  width: 18%;
  padding: 10px 0;
  margin: 0 auto;
  text-align: center;
  color: rgba(0,0,0,.1);
}

.nav-item a {
  color: rgba(0,0,0,.5);
  font-family: 'Roboto', sans-serif;
  font-size: 110%;
  padding: 20px;
}

a {
  text-decoration: none;
  color: #e8f1f2;
}

body {
  background-color: #e8f1f2;
}

