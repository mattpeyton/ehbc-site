}

.welcomeText {
  display: flex;
  flex-direction: row;
}

.headline {
  font-size: 100px;
  font-family: 'Roboto', sans-serif;
  color: #2f2e2e;
  text-shadow: #f4f4f4 0 0 3px;
}

#jesus {
  color: #f0efef;
  -webkit-text-stroke: 1px rgba(0,0,0,.1);
  text-shadow: rgba(255,157,0,.7) 0 0 5px;
  margin-left: 20px;
  margin-right: 20px;
}

#newButton a {
  font-size: 35px;
  font-family: 'Roboto', sans-serif;
